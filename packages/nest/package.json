{
  "name": "project",
  "version": "0.0.1",
  "description": "nest template for graphql api",
  "author": "Arjay Sitoy <sitoy.arjay@gmail.com>",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "test": "copyfiles src/api/graphql/types/** build && tsc && nyc ava",
    "pretest": "node -r ts-node/register node_modules/.bin/typeorm migration:run",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json",
    "prestart:prod": "rm -rf build && tsc",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "clean": "rm -rf build && rm -rf node_modules"
  },
  "dependencies": {
    "@nestjs/common": "^5.3.6",
    "@nestjs/core": "^5.3.6",
    "@nestjs/graphql": "^5.2.2",
    "@nestjs/typeorm": "^5.2.1",
    "apollo-server-express": "^2.0.6",
    "ava": "^5.1.0",
    "bcrypt": "^3.0.0",
    "chance": "^1.0.16",
    "fastify-formbody": "^2.0.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.1.1",
    "pg": "^7.4.3",
    "ramda": "^0.25.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "typeorm": "^0.2.8"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.0.0",
    "@types/bcrypt": "^2.0.0",
    "@types/chance": "^1.0.1",
    "@types/express": "^4.0.39",
    "@types/jest": "^23.3.4",
    "@types/node": "^10.11.5",
    "@types/ramda": "^0.26.0",
    "@types/supertest": "^2.0.4",
    "codecov": "^3.1.0",
    "copyfiles": "^2.1.0",
    "nyc": "^13.0.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-microsoft-contrib": "^5.1.0",
    "tsutils": "^3.0.0",
    "typescript": "^3.0.3"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
